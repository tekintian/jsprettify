{
  "name": "jsprettify",
  "version": "1.0.0",
  "description": "Prettify and decompress your JavaScript code with jsprettify",
  "main": "src/index.js",
  "bin": {
    "jsprettify": "./src/index.js"
  },
  "scripts": {
    "prettify": "node src/index.js",
    "build": "pkg ./src/index.js --targets node18-macos-x64 --output dist/jsprettify",
    "build:all": "bash build.sh"
  },
  "dependencies": {
    "prettier": "^2.8.8"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "pkg": {
    "outputPath": "dist",
    "assets": [
      "node_modules/prettier/**/*"
    ]
  }
  
}
