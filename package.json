{
  "name": "jsprettify",
  "version": "2.0.0",
  "description": "Prettify and decompress your JavaScript code with jsprettify",
  "main": "src/index.js",
  "bin": {
    "jsprettify": "./src/index.js"
  },
  "scripts": {
    "prettify": "node src/index.js",
